  <div id="dagens">
    <b>Dagens Zahabe:</b>
    <ol>
        
      {{#if daily.Story}}
        <div class='storyicon'><a href='#'><img src='images/read.png' alt='read full'></a></div>
        <a href='#'><li value='{{daily.cnt}}'><span>{{daily.Text}}</span></li></a>
      {{else}}
        <li value="{{daily.cnt}}">{{daily.Text}}</li>
      {{/if}}
      
    </ol>
  </div>
  <div id="rows">
    <ol reversed id="MVs">
      {{#each MVs}}
        {{#if this.Story}}
          <div class='storyicon'><a href='#'><img src='images/read.png' alt='read full'></a></div>
          <a href='#'><li value="{{this.cnt}}" class='MV'><span>{{this.Text}}</span></li></a>
        {{else}}
          <li class='MV' value="{{this.cnt}}" id="{{this.cnt}}">{{this.Text}}</li>
        {{/if}}
      {{/each}}
       
    </ol>

  </div>
